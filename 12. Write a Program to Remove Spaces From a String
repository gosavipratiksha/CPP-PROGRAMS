// C++ Program to remove spaces from a string#include <iostream>
#include <string>

using namespace std;

string remove_spaces(string str) {
    string result = "";
    for (size_t i = 0; i < str.length(); ++i) {
        if (str[i] != ' ') {
            result += str[i];
        }
    }
    return result;
}

int main() {
    string str = "Gfg to the moon";

    cout << "Without spaces: " << remove_spaces(str) << endl;

    return 0;
}
