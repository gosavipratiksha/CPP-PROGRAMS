//WRITE A PROGRAM TO CHECK WHETHER A CHARCHTER IS VOWEL OR CONSTANT
#include<iostream>
using namespace std;
int main()
{
	char ch;
	cout<<"Enter a charachter: "<<endl;
	cin>>ch;
	if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||
	ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
	{
		cout<<ch<<" is a vowel."<<endl;
	}
	else
	{
		cout<<ch<<" is a consatnt."<<endl;
	}
	return 0;
}
